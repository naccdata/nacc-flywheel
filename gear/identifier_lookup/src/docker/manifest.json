{
    "name": "identifier-lookup",
    "label": "NACC ID Lookup",
    "description": "Gear to check for NACC ID for incoming data",
    "version": "0.1.0",
    "author": "NACC",
    "maintainer": "NACC <nacchelp@uw.edu>",
    "cite": "",
    "license": "MPL-2.0",
    "url": "https://naccdata.github.io/flywheel-gear-extensions",
    "source": "https://github.com/naccdata/flywheel-gear-extensions",
    "environment": {
        "FLYWHEEL": "/flywheel/v0"
    },
    "custom": {
        "gear-builder": {
            "category": "utility",
            "image": "naccdata/identifier-lookup:0.1.0"
        },
        "flywheel": {
            "suite": "NACC Data Gears",
            "show-job": true
        }
    },
    "inputs": {
        "api-key": {
            "base": "api-key"
        },
        "input_file": {
            "description": "The CSV input file. Each row must have ADCID and PTID",
            "base": "file",
            "type": {
                "enum": [
                    "tabular data"
                ]
            }
        }
    },
    "config": {
        "dry_run": {
            "description": "Whether to do a dry run",
            "type": "boolean",
            "default": false
        },
        "admin_group": {
            "description": "Name of the admin group",
            "type": "string",
            "default": "nacc"
        },
        "database_mode": {
            "description": "Whether to create identifiers in dev or prod database",
            "type": "string",
            "default": "prod"
        },
        "apikey_path_prefix": {
            "description": "The instance specific AWS parameter gearbot path prefix",
            "type": "string",
            "default": "/prod/flywheel/gearbot"
        },
        "date_field": {
            "description": "Variable name of the visit date field for the module",
            "type": "string",
            "default": "visitdate"
        }
    },
    "command": "/bin/run"
}